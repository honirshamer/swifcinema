        <%#= form2.text_field :id %>
  
        <%#= form2.text_field :id_movie %>
  
        <%#= form2.text_field :price %>

      </div>-->
      <% @a=@a+1 %>
  <% end %>
  Available <%= @count %>
  <br><br>
  <div class="actions">
    <%= form2.submit("Submit") %>
  </div>
<% end %>
