<h1>Swiftlet Cinema</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Time</th>
      <th>Duration</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tmp="" %>
    <% @admins.each do |admin| %>
      <% if @tmp!=admin.id_movie %>
      <tr>
        
        <td><%= admin.name %></td>
        <td><%= admin.time %></td>
        <td><%= admin.duration %></td>
        <td><%= admin.price %></td>
        <td><%= link_to 'Seat', admins_seat_path(admin) %></td>
      </tr>
      <% end %>
      <% @tmp=admin.id_movie %>
    <% end %>
  </tbody>
</table>

<br>


